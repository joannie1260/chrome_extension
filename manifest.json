{
    "manifest_version": 3,
    "name": "Vicky",
    "description": "This extension makes an API call and gets data.",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "https://your-api-url.com/*"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}